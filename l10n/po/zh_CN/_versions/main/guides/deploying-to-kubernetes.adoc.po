msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/deploying-to-kubernetes.adoc:21
msgid "an IDE"
msgstr "IDE"

#. type: Plain text
#: upstream/_versions/main/guides/deploying-to-kubernetes.adoc:22
msgid "JDK 11+ installed with `JAVA_HOME` configured appropriately"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/deploying-to-kubernetes.adoc:23
msgid "Apache Maven {maven-version}"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/deploying-to-kubernetes.adoc:24
